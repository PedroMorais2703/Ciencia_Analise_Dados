import matplotlib.pyplot as plt
import pandas as pd
import seaborn as sns

df = pd.read_csv('ecommerce_preparados.csv')

#historograma parametro
plt.figure(figsize = (10,6))
plt.hist(df['Preço'], bins = 100, color = 'green', alpha = 0.8)
plt.title('Historograma - Distribuição de Preços em relação aos Vendidos')
plt.xlabel('Preço')
plt.xticks(ticks=range(0, int(df['Preço'].max()) + 100, 100))
plt.ylabel('Qtd_Vendidos')
plt.grid(True)
plt.show()

#grafico de dispersão
plt.scatter(df['Preço'], df['Qtd_Vendidos'], color='#5883a8', alpha = 0.6, s=30)
plt.title('Dispersão - Preços e Quantidade de Vendidos')
plt.xlabel('Qtd_Vendidos')
plt.ylabel('Preço')
plt.show()

# mapa calor
corr = df[['Preço', 'Nota_MinMax']].corr()
sns.heatmap(corr, annot=True, cmap='coolwarm')
plt.title('Corelação - Preços e Nota MinMax')
plt.show()

#grafico barra
plt.figure(figsize=[40,10])
df['Gênero'].value_counts().plot(kind='bar', color='blue')
plt.title('Nivel de escolaridade 1')
plt.xlabel('Gnero')
plt.ylabel('Qtd_Vendidos')
plt.xticks(rotation=0)
plt.show()

#grafico pizza
x = df['Gênero'].value_counts().index
y = df['Gênero'].value_counts().values
plt.figure(figsize=[10,6])
plt.pie(y, labels=x, autopct='%.1f%%', startangle=90)
plt.title('Distribuição dos Gêneros')
plt.show()

#grafico dencidade
plt.figure(figsize=[10,6])
sns.kdeplot(df['Preço'], fill=True, color='blue')
plt.title('Densidade Preços')
plt.xlabel('Preço')
plt.show()


# gráfico de regressão
plt.figure(figsize=(10, 6))
sns.regplot(x='Preço', y='Nota_MinMax', data=df,color='green',scatter_kws={'alpha':0.5, 'color':'red', 's':50})
plt.title('Regressão: Nota_MinMax por Preço')
plt.xlabel('Preço')
plt.ylabel('Nota_MinMax')
plt.grid(True, alpha=0.3)
plt.show()

